                <Button x:ConnectionId='496' x:Name="btnSave" Width="100" Content="SAVE"></Button>
            </StackPanel>
        </Border>
        <ListView x:ConnectionId='3' x:Name="lvElements" SelectedItem="{Binding SelectedElement, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" RelativePanel.Below="spOptions" RelativePanel.AlignBottomWithPanel="True" RelativePanel.AlignLeftWithPanel="True" Width="200" CornerRadius="10" Background="AliceBlue" BorderThickness="0.5" BorderBrush="Black" ItemsSource="{Binding ElementsSprite}">
