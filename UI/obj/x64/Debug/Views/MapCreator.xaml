            <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                <Button x:ConnectionId='491' x:Name="btnBack"  VerticalAlignment="Top"></Button>
                <TextBlock x:ConnectionId='492' x:Name="tblMapName" VerticalAlignment="Center" Text="MAP NAME:" HorizontalAlignment="Center"></TextBlock>
                <TextBox x:ConnectionId='493' x:Name="tbMapName" Text="{Binding EmptyMap.Name,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="200" Margin="10" HorizontalAlignment="Center"></TextBox>
                <TextBlock x:ConnectionId='494' x:Name="tblMapCreator" VerticalAlignment="Center" Text="MAP CREATOR:"></TextBlock>
