                <Button x:Name="btnMinSize" Margin="180, 10">16x16</Button>
                <Button x:Name="btnMedSize" RelativePanel.RightOf="btnMinSize" Margin="180, 10">24x16</Button>
                <Button x:Name="btnMaxSize" RelativePanel.RightOf="btnMedSize" Margin="180, 10">30x16</Button>
