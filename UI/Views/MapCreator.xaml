                <Button x:Name="btnBack"  VerticalAlignment="Top"></Button>
                <TextBlock x:Name="tblMapName" VerticalAlignment="Center" Text="MAP NAME:" HorizontalAlignment="Center"></TextBlock>
                <TextBox x:Name="tbMapName" Text="{Binding EmptyMap.Name,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="200" Margin="10" HorizontalAlignment="Center"></TextBox>
                <TextBlock x:Name="tblMapCreator" VerticalAlignment="Center" Text="MAP CREATOR:"></TextBlock>
                <TextBox x:Name="tbMapCreator" Text="{Binding EmptyMap.Nick,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" Width="200" Margin="10"></TextBox>
                <Button x:Name="btnSave" Width="100" Content="SAVE"></Button>
            </StackPanel>
        </Border>
        <ListView x:Name="lvElements" SelectedItem="{Binding SelectedElement, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" RelativePanel.Below="spOptions" RelativePanel.AlignBottomWithPanel="True" RelativePanel.AlignLeftWithPanel="True" Width="200" CornerRadius="10" Background="AliceBlue" BorderThickness="0.5" BorderBrush="Black" ItemsSource="{Binding ElementsSprite}">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <Image Source="{Binding Imagen, UpdateSourceTrigger=PropertyChanged}" Margin="30" Height="100" Width="100"/>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
